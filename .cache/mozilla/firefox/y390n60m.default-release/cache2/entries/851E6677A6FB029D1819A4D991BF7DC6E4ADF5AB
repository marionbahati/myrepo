    this.beziehungen = [];
  }
  ngOnInit() {
    if (this.data.isReady()) {
      this.draw();
    } else {
      this.data.ready$.pipe(first()).subscribe({
        next: () => this.draw()
      });
    }
  }
  relationChanged(event) {
    this.addFilterAndCreateLinks(event);
  }
  getAvailableCompanies() {
    this.available_companies = this.data.getCompanies().map((c) => c.name);
  }
