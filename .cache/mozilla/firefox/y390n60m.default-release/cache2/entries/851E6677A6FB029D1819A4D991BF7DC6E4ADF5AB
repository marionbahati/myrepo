  createNodes() {
    this.nodes = this.data.getCompanies().map((c, i, arr) => {
      return { index: i, name: c.name, group: 0 };
    });
  }
  // createNodes(){
  //   this.nodes = this.service.getCompanies().map(
  //     (c: Company, i: number, arr: Company[]) => {
