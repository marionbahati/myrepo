// src/app/services/graph-data-adapter.service.ts
var _GraphDataAdapterService = class _GraphDataAdapterService {
  constructor(graphData) {
    this.graphData = graphData;
    this.onDataConstructed$ = new Subject2();
    this.is_constructed = false;
    this.nodes = [];
    this.links = [];
    this.graphData.fetchData();
    this.ready$ = this.graphData.ready$;
    this.graphData.ready$.subscribe(() => {
      this.setupAllData();
    });
  }
  isConstructed() {
    return this.is_constructed;
  }
  setupAllData() {
    this.is_constructed = false;
    this.createNodes();
    this.createAllLinks();
    this.onDataConstructed$.next(true);
    this.is_constructed = true;
  }
  getNodes() {
    return this.nodes;
  }
  getLinks() {
    return this.links;
  }
  isReady() {
    return this.graphData.isReady();
  }
  getCompanies() {
    return this.graphData.companies;
  }
  createNodes() {
    this.nodes = this.graphData.getCompanies().map((c, i, arr) => {
      return { index: i, name: c.name, group: 0 };
    });
  }
  getNodeByName(n) {
    return this.nodes.find((v) => v.name == n);
  }
  /**
   * Gets all nodes and links for a given company that are connected through the given relation (or all if relation is empty)
   * @param company The name (as a string) of the company we are centering
   * @param relation The relation that connects the company to its "related" companies
   * @returns
   */
  getRelationGraphForCompany(company, relation) {
    if (company == void 0 || !this.getAvailableCompanyNames().includes(company)) {
      throw new Error("Invalid company: " + company);
    }
    let originalCompany = this.graphData.companies.find((c) => c.name == company);
    if (originalCompany == void 0) {
      throw new Error("Company not found in dataset.");
    }
    let originalCompanyNode = this.getNodeByName(originalCompany.name);
    let originalRelations = originalCompany.Relations.filter((r) => r.relation_type == relation || relation == "");
    let relatedObjectIds = originalRelations.map((r) => r.ObjectID).filter((r) => r !== void 0);
    let relatedCompanies = this.graphData.companies.filter((c) => relatedObjectIds.includes(c.id));
    let nodes = [originalCompanyNode, ...relatedCompanies.map((c, i) => this.getNodeByName(c.name))];
    let links = [];
    for (let relation2 of originalRelations) {
      let link = {
        source: originalCompanyNode,
        target: this.getNodeById(relation2.ObjectID)
      };
      if (link.target && link.source) {
        if (!this.hasLink(links, link)) {
          links.push(link);
        }
      }
    }
    return {
      nodes,
      links
    };
  }
  getAvailableCompanyNames() {
    return this.graphData.getCompanies().map((c) => c.name);
  }
  getAvailableRelations() {
    let relations = [];
    for (let c of this.getCompanies()) {
      for (let relation of c.Relations) {
        if (!relations.includes(relation.relation_type)) {
          relations.push(relation.relation_type);
        }
      }
    }
    return relations;
  }
  getNodeById(id) {
    let company = this.graphData.getCompanies().find((x) => x.id == id);
    if (company) {
      return this.getNodeByName(company.name);
    }
    return void 0;
  }
  // check if a link ("link") is in an array of links "list"
  hasLink(list, link) {
    for (let l of list) {
      if (link.source == l.source && link.target == l.target) {
        return true;
      }
    }
    return false;
  }
  createAllLinks() {
    let links = [];
    let centralCompany = this.graphData.getCompanies().find((company) => company.name === "Codeschaffer");
    if (!centralCompany) {
      return;
    }
    let centralNode = this.getNodeByName(centralCompany.name);
    for (let company of this.graphData.getCompanies()) {
      if (company.name !== "Codeschaffer") {
        let targetNode = this.getNodeByName(company.name);
        if (targetNode && centralNode) {
          let link = {
            source: centralNode,
            target: targetNode
          };
          if (!this.hasLink(links, link)) {
            links.push(link);
          }
        }
      }
    }
    this.links = links;
  }
};
