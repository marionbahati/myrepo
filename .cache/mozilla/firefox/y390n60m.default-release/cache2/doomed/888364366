    this.ready$ = new Subject();
  }
  fetchData() {
    console.log("constructing GraphData-Service");
    this.loadCompanies().then((c) => {
      this.companies = c;
      this.ready$.next(true);
    });
  }
  isReady() {
    return this.companies.length > 0;
  }
  getCompanies() {
    return this.companies;
  }
  getCompanyNames() {
    return this.companies.map((x) => x.name);
  }
  loadCompanies() {
