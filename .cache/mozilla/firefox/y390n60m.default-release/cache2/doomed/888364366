var _NewgraphComponent = class _NewgraphComponent {
  constructor(graphDataAdapter) {
    this.graphDataAdapter = graphDataAdapter;
    this.margin = 20;
    this.width = 1200 - this.margin * 2;
    this.height = 900 - this.margin * 2;
    this.selected_relation = "";
    this.selected_company = "Codeschaffer";
    this.d3Data = {
      node: void 0,
      link: void 0,
      simulation: void 0,
      text: void 0
    };
    this.dragged = (event, d) => {
      d.fx = event.x;
      d.fy = event.y;
    };
    this.dragended = (event, d) => {
      if (!event.active)
        this.d3Data.simulation.alphaTarget(0);
      d.fx = null;
      d.fy = null;
    };
    this.dragstarted = (event, d) => {
      if (!event.active)
        this.d3Data.simulation.alphaTarget(0.3).restart();
      d.fx = d.x;
      d.fy = d.y;
    };
    this.ticked = () => {
      this.d3Data.simulation.nodes().forEach((node) => {
        if (node.name === this.selected_company) {
        }
      });
      this.d3Data.node.attr("cx", (d) => d.x).attr("cy", (d) => d.y);
      this.d3Data.link.attr("x1", (d) => {
        return d.source.x * 1;
      }).attr("y1", (d) => d.source.y * 1).attr("x2", (d) => d.target.x * 1).attr("y2", (d) => d.target.y * 1);
      this.d3Data.text.attr("dx", (d) => d.x * 1).attr("dy", (d) => d.y * 1);
    };
    this.relationsStringList = [];
  }
  /**
   * make graphAdapter accessible in HTML-Template
   **/
  graphAdapter() {
    return this.graphDataAdapter;
  }
  /**
   * If data already available, draw it, else, wait for it to become available and draw then.
   **/
  ngOnInit() {
    if (this.graphDataAdapter.isConstructed()) {
      this.dataUpdated();
    } else {
      this.graphDataAdapter.onDataConstructed$.pipe(first()).subscribe({
        next: () => this.dataUpdated()
      });
    }
  }
  /**
   * Update data on changes to the relation-selection
   **/
  relationChanged(event) {
    this.dataUpdated();
  }
  setRelation(ev) {
    this.selected_relation = this.getStringFromEvent(ev);
    this.dataUpdated();
  }
  getStringFromEvent(ev) {
    return ev.target["value"];
  }
  setCompany(ev) {
    this.selected_company = this.getStringFromEvent(ev);
    this.dataUpdated();
  }
  createSvg() {
    this.svg = d3.select("figure#netgraph").append("svg").attr("width", this.width + this.margin * 2).attr("height", this.height + this.margin * 2).append("g");
  }
  drawGraph(nodes, links) {
    this.createSvg();
    this.d3Data.simulation = d3.forceSimulation(nodes).force("charge", d3.forceManyBody().strength(-300)).force("center", d3.forceCenter(this.width / 2, this.height / 2)).force("link", d3.forceLink(links).id((d) => d.index)).force("collision", d3.forceCollide().radius(30)).force("link", d3.forceLink(links).id((d) => d.index).distance(250)).on("tick", this.ticked);
    this.d3Data.link = this.svg.append("g").selectAll("line").data(links).enter().append("line").attr("stroke-width", 2).style("stroke", "orange");
    this.d3Data.node = this.svg.append("g").selectAll("circle").data(nodes).enter().append("circle").attr("r", 25).attr("fill", (d, i) => d3.schemeCategory10[i % 10]).call(d3.drag().on("start", (e, d) => this.dragstarted(e, d)).on("drag", (e, d) => this.dragged(e, d)).on("end", (e, d) => this.dragended(e, d)));
    this.d3Data.text = this.svg.append("g").selectAll("text").data(nodes).enter().append("text").style("stroke", (d, i) => d3.schemeCategory10[i % 10]).text((d) => d.name);
    this.d3Data.simulation.nodes().forEach((node) => {
      if (node.name === this.selected_company) {
        node.fx = this.width / 2;
        node.fy = this.height / 2;
      }
    });
  }
  deleteSearchterm() {
    this.selected_company = "";
    this.dataUpdated();
  }
  displaySearchTermRelations() {
    this.relationsStringList = [];
    const searchedNode = this.graphDataAdapter.getNodes().find((node) => node.name.toLowerCase() === this.selected_company.toLowerCase());
    for (let company of this.graphDataAdapter.getCompanies()) {
      if (company.name.toLowerCase() === this.selected_company.toLowerCase()) {
        for (let relation of company.Relations ?? []) {
          let targetCompany = this.graphDataAdapter.getNodeById(relation.ObjectID);
          if (targetCompany) {
            this.relationsStringList.push(`${relation.relation_type}  : ${targetCompany.name}`);
          }
        }
      }
    }
    if (this.relationsStringList.length === 0) {
      this.relationsStringList.push(`${this.selected_company} ist ein nur kunde von Codeschaffer.Weitere bekannte Beziehungen gibt es nicht`);
    }
  }
  dataUpdated() {
    let graph = this.graphDataAdapter.getRelationGraphForCompany(this.selected_company, this.selected_relation);
    this.displaySearchTermRelations();
    d3.select("figure#netgraph").selectAll("*").remove();
    this.drawGraph(graph.nodes, graph.links);
  }
};
